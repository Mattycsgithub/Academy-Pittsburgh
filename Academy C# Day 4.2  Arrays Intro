string name1 = "John";
            string name2 = "Jean";
            string name3 = "Saturn";

            Console.WriteLine(name1);
            Console.WriteLine(name2);
            Console.WriteLine(name3);

            string[] names = new string[3];
            names[0] = "John";
            names[1] = "Jean";
            names[2] = "Saturn";

            Console.WriteLine(names[0]);
            Console.WriteLine(names[1]);
            Console.WriteLine(names[2]);
